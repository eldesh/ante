
trait Foo a with
    traitfn: a -> a

impl Foo (Maybe a) given Foo a with
    traitfn m =
        match m
        | Some x -> Some <| traitfn x
        | None -> None

impl Foo I32 with
    traitfn x = x



inner x =
    traitfn x

outer x = inner x

outer (Some 3i32)

